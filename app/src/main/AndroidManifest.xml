<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sociogram">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/socio"
        android:label="Sociogram"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">




        <activity android:name=".FriendlyMessage" />

        <activity android:name="com.example.sociogram.MainActivity"    android:label="@string/app_name"/>



        <activity android:name=".SignInActivity"    android:label="@string/app_name">  <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>

    </application>

</manifest>